{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"lib": ["dom", "es5", "es2016", "es2017.object"],
		"module": "esnext",
		"noImplicitAny": true,
		"outDir": "./build/",
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"target": "es5"
	},
	"include": ["./src/**/**/*"]
}
